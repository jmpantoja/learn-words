{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cocur/slugify": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "laminas/laminas-filter": "^2.9",
        "league/tactician-bundle": "^1.2",
        "league/tactician-doctrine": "^1.1",
        "msgphp/domain": "^0.15.1",
        "myclabs/php-enum": "^1.7",
        "nesbot/carbon": "^2.36",
        "overblog/graphql-bundle": "^0.13.3",
        "ramsey/uuid": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.71",
        "sonata-project/doctrine-extensions": "^1.9",
        "sonata-project/doctrine-orm-admin-bundle": "^3.20",
        "sonata-project/form-extensions": "^1.6",
        "sonata-project/twig-extensions": "^1.4",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/dom-crawler": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/stopwatch": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "behat/mink": "^1.8",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3",
        "dama/doctrine-test-bundle": "^6.3",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "mikey179/vfsstream": "^1.6",
        "phpspec/phpspec": "^6.1",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/PlanB/EdgeBundle/Resources/functions/datatype.php",
            "src/PlanB/EdgeBundle/Resources/functions/text.php"
        ],
        "psr-4": {
            "PlanB\\": "src/PlanB/",
            "LearnWords\\": "src/LearnWords/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
