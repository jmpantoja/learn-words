<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="LearnWords\Domain\Dictionary\Vocabulary" table="vocabulary">
        <field name="relevance" type="Relevance"/>
        <embedded name="wording" class="LearnWords\Domain\Dictionary\Wording" use-column-prefix="false"/>
        <embedded name="example" class="LearnWords\Domain\Dictionary\Example" use-column-prefix="false"/>
    </entity>
</doctrine-mapping>
