parameters:
	level: 6
	paths:
	    - src


	ignoreErrors:
	    - '#invoked with [0-9]+ parameter[s]?, [0-9]+ required#'
	    - '#Empty array passed to foreach#'
	    - '#Return type .* of method .* should be compatible with return type \(true\) of method .*#'

	reportUnmatchedIgnoredErrors: false

	checkMissingIterableValueType: false

	checkGenericClassInNonGenericObjectType: false


